	Write !,"<span class=ButtonDiv ID="""" style=""cursor:hand;width:300"" onmouseover=""this.className='ButtonDivHover'"" onmouseout=""this.className='ButtonDiv'"" onclick=""CloseSelectBlock()"" TITLE=""""><center>Отмена</center></span>"
	Write "</center>",!
