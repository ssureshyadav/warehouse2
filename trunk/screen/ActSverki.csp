<script language="cache" runat="server">

 s AllowedDocs=$LB("Docs.Platejka","Docs.Rash","Docs.Otchet")
 
 // выбрать все документы этого контрагента, в том числе платёжки от этого контрагента к нам
 &sql(declare zz cursor for 
 	SELECT ID,Name,Dat,Tim,Summa,DocumentClass,DocumentClass->Name
 	INTO :ID,:Name,:Dat,:Tim,:Summa,:DocumentClass,:DocumentClassName
 	FROM Docs.Super
 	WHERE Kontr=:KontrId
 	and Dat>=:date1 and Dat<=:date2)
 &sql(open zz)
 f  &sql(fetch zz) q:SQLCODE  d
 . //пропускаем документы не из списка AllowedDocs
 . s return=0	//boolean переменная - определяет, стоит ли пропустить эту итерацию
 . if '$LF(AllowedDocs,DocumentClass) s return =1
 . k RubSumma
 . //если расходная накладная, то извлечём рублёвую сумму
 . if (DocumentClass="Docs.Rash") d
 . . s obj=##class(Docs.Rash).%OpenId(ID)
 . . if '$ISOBJECT(obj) q
 . . s RubSumma=obj.RubSumma
 . . if '$ISOBJECT(obj.opl) q
 . . if obj.opl.%Id()=1 s return =1	//чейта???
 . //если платёжка - вытащим рублёвую сумму
 . if (DocumentClass="Docs.Platejka") d
 . . s obj=##class(Docs.Platejka).%OpenId(ID)
 . . if '$ISOBJECT(obj) q
 . . s RubSumma=obj.RubSumma
 . //если технический отчёт то попадёт только если получено бумажное подверждение
 . if (DocumentClass="Docs.Otchet") d
 . . s obj=##class(Docs.Otchet).%OpenId(ID)
 . . if '$ISOBJECT(obj) q
 . . if 'obj.GotPaperProve s return =1
 . q:return
 . s Docs(1,Dat,Tim,ID,"ID")=ID
 . s Docs(1,Dat,Tim,ID,"Name")=Name
 . s Docs(1,Dat,Tim,ID,"Dat")=Dat
 . s Docs(1,Dat,Tim,ID,"Summa")=$G(RubSumma,Summa)
 . s Docs(1,Dat,Tim,ID,"DocumentClass")=DocumentClass
 . s Docs(1,Dat,Tim,ID,"DocumentClassName")=DocumentClassName
 . s Docs(1,Dat,Tim,ID,"Kontr")=KontrId
 &sql(close zz)
 
 
 // выбрать платёжки от нас к этому контрагенту
 &sql(declare zz1 cursor for 
 	SELECT ID,Name,Dat,Tim,RubSumma,DocumentClass,DocumentClass->Name,Kontr
 	INTO :ID,:Name,:Dat,:Tim,:RubSumma,:DocumentClass,:DocumentClassName,:Kontr
 	FROM Docs.Platejka
 	WHERE Poluh=:KontrId and Kontr=:OurCompany
 	and Dat>=:date1 and Dat<=:date2)
 &sql(open zz1)
 f  &sql(fetch zz1) q:SQLCODE  d
 . //если это расходная, то извлечём рублёвую сумму
 . s Docs(1,Dat,Tim,ID,"ID")=ID
 . s Docs(1,Dat,Tim,ID,"Name")=Name
 . s Docs(1,Dat,Tim,ID,"Dat")=Dat
 . s Docs(1,Dat,Tim,ID,"Summa")=RubSumma
 . s Docs(1,Dat,Tim,ID,"DocumentClass")=DocumentClass
 . s Docs(1,Dat,Tim,ID,"DocumentClassName")=DocumentClassName
 . s Docs(1,Dat,Tim,ID,"Kontr")=Kontr
 . s Docs(1,Dat,Tim,ID,"Poluh")=KontrId
 &sql(close zz1)
 
 /*w "<!-- ",!
 //zw Docs
 w " -->",!*/
 
 
 //цикл по выбранным документам
 s dt="" f  s dt=$O(Docs(1,dt)) q:dt=""  d
 . s tm="" f  s tm=$O(Docs(1,dt,tm)) q:tm=""  d
 . . s (cred,deb)=""
 . . s DocId=$O(Docs(1,dt,tm,""))
 . . s name = Docs(1,dt,tm,DocId,"Name")
 . . s DocumentClass = Docs(1,dt,tm,DocId,"DocumentClass")
 . . s DocumentClassName = Docs(1,dt,tm,DocId,"DocumentClassName")
 . . s date = Docs(1,dt,tm,DocId,"Dat")
 . . s summa=Docs(1,dt,tm,DocId,"Summa")
 . . s Kontr=Docs(1,dt,tm,DocId,"Kontr")
 . . s Poluh=$G(Docs(1,dt,tm,DocId,"Poluh"))
 . . if (DocumentClass="Docs.Rash") s cred="",deb=summa		//платная расходная накладная
 . . if (DocumentClass="Docs.Otchet") s cred=summa,deb=""	//технический отчёт
