<script language="cache" runat="server">
 
 s dat1=%request.Get("dat1")
 s dat2=%request.Get("dat2")
 
 s date1=$S(dat1="":"",1:$ZDH(dat1,4))
 s date2=$S(dat2="":"",1:$ZDH(dat2,4))

 s KontrId = $g(%request.Data("kontr",1)), OurCompany=$g(%request.Data("OurCompany",1))
 if KontrId {
	 s KontrObj = ##class(Common.Kontragent).%OpenId(KontrId)
	 s KontrName = KontrObj.Name, KontrDir = KontrObj.chief
	 
	 if (+OurCompany) {
		 s OurCompanyObj = ##class(Common.Kontragent).%OpenId(OurCompany)
	 } else  {
		 s OurCompanyObj = KontrObj.OurCompany
	 }
	 
	 if $ISOBJECT(OurCompanyObj)  {
		 s OurName =OurCompanyObj.Name, OurDir = OurCompanyObj.chief
	 }
 }
 
 s ok=##class(Account.Kontragent).Get(KontrId,dat1,.saldo)
 if '+ok s saldo=$J(0,"",2)
 if (saldo<0) {
	 s SaldoDeb=saldo, SaldoCred=""
 } else {
	 s SaldoCred=saldo, SaldoDeb=""
 }
</script><html xmlns:v="urn:schemas-microsoft-com:vml"
