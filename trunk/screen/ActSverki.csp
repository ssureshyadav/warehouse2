<script language="cache" runat="server">

 
 #def1arg gl(%keys) ^CacheTemp.ActSverki($P,%keys)
 k $$$gl(1)
 // выбрать все документы этого контрагента, в том числе платёжки от этого контрагента к нам
 &sql(declare zz cursor for 
 	SELECT ID,Name,Dat,Tim,Summa,DocumentClass,DocumentClass->Name
 	INTO :ID,:Name,:Dat,:Tim,:Summa,:DocumentClass,:DocumentClassName
 	FROM Docs.Super
 	WHERE Kontr=:KontrId
 	and Dat>=:date1 and Dat<=:date2 and Stat=2 
 	and DocumentClass In ('Docs.Platejka','Docs.Rash','Docs.Otchet')
 	)
 	
 &sql(open zz)
 f { &sql(fetch zz) q:SQLCODE  
  s return=0	//boolean переменная - определяет, стоит ли пропустить эту итерацию
  k RubSumma
  ;если расходная накладная, то извлечём рублёвую сумму
  if (DocumentClass="Docs.Rash") {
	  s exist=##class(Docs.Rash).%ExistsId(ID)
	  s:exist RubSumma=##class(Docs.Rash).RubSummaGetStored(ID)
	  s opl="" s:exist opl=##class(Docs.Rash).oplGetStored(ID) 
	  if opl=1 s return =1 ; оплачено по гарантии не учитывать
  }
  ;если платёжка - вытащим рублёвую сумму
  if (DocumentClass="Docs.Platejka") {
  	s exist=##class(Docs.Platejka).%ExistsId(ID) 
  	if exist 
  	{
	  	s PoluhObj = ##class(Docs.Platejka).%OpenId(ID)	//документ Платёжное Поручение
	  	if ($ISOBJECT(PoluhObj.Poluh)) s PoluhId = PoluhObj.Poluh.%Id()
	  	//if PoluhId'=OurCompany s return=1
	  	s RubSumma=##class(Docs.Platejka).RubSummaGetStored(ID)
  	}
  } 
  ;если технический отчёт то попадёт только если получено бумажное подверждение
  if (DocumentClass="Docs.Otchet") {
   s exist=##class(Docs.Otchet).%ExistsId(ID) 
   s prove=""  s:exist prove=##class(Docs.Otchet).GotPaperProveGetStored(ID) 
   s:'prove return=1
  }
  continue:return
  ;накапливаем
  s $$$gl(1,Dat,Tim,ID,"ID")=ID
  s $$$gl(1,Dat,Tim,ID,"Name")=Name
  s $$$gl(1,Dat,Tim,ID,"Dat")=Dat
  s $$$gl(1,Dat,Tim,ID,"Summa")=$G(RubSumma,Summa)
  s $$$gl(1,Dat,Tim,ID,"DocumentClass")=DocumentClass
  s $$$gl(1,Dat,Tim,ID,"DocumentClassName")=DocumentClassName
  s $$$gl(1,Dat,Tim,ID,"Kontr")=KontrId
 } &sql(close zz)
 
 
 // выбрать платёжки от нас к этому контрагенту
 &sql(declare zz1 cursor for 
 	SELECT ID,Name,Dat,Tim,RubSumma,DocumentClass,DocumentClass->Name,Kontr
 	INTO :ID,:Name,:Dat,:Tim,:RubSumma,:DocumentClass,:DocumentClassName,:Kontr
 	FROM Docs.Platejka
 	WHERE Poluh=:KontrId and Kontr=:OurCompany and Stat=2
 	and Dat>=:date1 and Dat<=:date2)
 &sql(open zz1)
 f { &sql(fetch zz1) q:SQLCODE  
  ;если это расходная, то извлечём рублёвую сумму
  s $$$gl(1,Dat,Tim,ID,"ID")=ID
  s $$$gl(1,Dat,Tim,ID,"Name")=Name
  s $$$gl(1,Dat,Tim,ID,"Dat")=Dat
  s $$$gl(1,Dat,Tim,ID,"Summa")=RubSumma
  s $$$gl(1,Dat,Tim,ID,"DocumentClass")=DocumentClass
  s $$$gl(1,Dat,Tim,ID,"DocumentClassName")=DocumentClassName
  s $$$gl(1,Dat,Tim,ID,"Kontr")=Kontr
  s $$$gl(1,Dat,Tim,ID,"Poluh")=KontrId
 } &sql(close zz1)
 
 /*w "<!-- ",!
 //zw Docs
 w " -->",!*/
 #define fn(%num) $s(+%num=0:"",1:$j(+%num,"",2)) 
 
 //цикл по выбранным документам
 s dt="" f  s dt=$O($$$gl(1,dt)) q:dt=""  d
 . s tm="" f  s tm=$O($$$gl(1,dt,tm)) q:tm=""  d
 . . s (cred,deb)=""
 . . s DocId=$O($$$gl(1,dt,tm,""))
 . . s name = $$$gl(1,dt,tm,DocId,"Name")
 . . s DocumentClass = $$$gl(1,dt,tm,DocId,"DocumentClass")
 . . s DocumentClassName = $$$gl(1,dt,tm,DocId,"DocumentClassName")
 . . s date = $$$gl(1,dt,tm,DocId,"Dat")
 . . s summa=$$$gl(1,dt,tm,DocId,"Summa")
 . . s Kontr=$$$gl(1,dt,tm,DocId,"Kontr")
 . . s Poluh=$G($$$gl(1,dt,tm,DocId,"Poluh"))
 . . if (DocumentClass="Docs.Rash") s cred=summa,deb=""		//платная расходная накладная
 . . if (DocumentClass="Docs.Otchet") s cred=summa,deb=""	//технический отчёт
 . . if (DocumentClass="Docs.Platejka"),Kontr=KontrId s cred=summa,deb=""	//входящие платёжки
 . . if (DocumentClass="Docs.Platejka"),Poluh=KontrId s cred="",deb=summa	//исходящие платёжки
 . . s deb=$J(deb,"",2) 
 . . s cred=$J(cred,"",2)
 . . 
 . . s DebAll=$J($G(DebAll)+deb,"",2)
 . . s CredAll=$J($G(CredAll)+cred,"",2)
 . . s:+date date = $ZD(date,4)
 . . &html<<!--height=15 style='mso-height-source:userset;height:11.85pt'-->
 <tr>
  <td></td>
  <td class=xl49>#($$$dtru(date))#</td>
  <td colspan=2 class=xl49 style="white-space:normal"><a href="../print.csp?id=#(DocId)#&class=#(DocumentClass)#" target=_blank>#(name)#</a> (#(DocumentClassName)#)</td>
  <td colspan=2 class=xl50>#($$$fn(deb))#</td>
  <td colspan=3 class=xl50>#($$$fn(cred))#</td>
  <td class=xl49></td>
  <td colspan=2 class=xl49></td>
  <td colspan=2 class=xl49></td>
  <td colspan=2 class=xl49></td>
 </tr>
 >
 k $$$gl(1)
 
 s endSaldo = saldo + $G(DebAll) - $G(CredAll)
 s (EndSaldoDeb,EndSaldoCred)="" 
 s:endSaldo>0 EndSaldoDeb=endSaldo
 s:endSaldo<0 EndSaldoCred=endSaldo*-1
 
</script>
 
 <tr height=15 style='mso-height-source:userset;height:11.85pt'>
  <td height=15 style='height:11.85pt'></td>
